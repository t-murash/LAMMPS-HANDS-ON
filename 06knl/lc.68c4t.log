  32768 atoms
  read_restart CPU = 0.0587962 secs
replicate       $x $y $z
replicate       4 $y $z
replicate       4 2 $z
replicate       4 2 2
  orthogonal box = (3.48762 3.48762 3.48762) to (187.619 95.5534 95.5534)
  17 by 2 by 2 MPI processor grid
  524288 atoms
  replicate CPU = 0.0604961 secs

compute		rot all temp/asphere
group		spheroid type 1
524288 atoms in group spheroid
variable	dof equal count(spheroid)+3
compute_modify	rot extra ${dof}
compute_modify	rot extra 524291

pair_style	gayberne/intel 1.0 3.0 1.0 4.0
pair_coeff	1 1 1.0 1.0 1.0 0.5 0.2 1.0 0.5 0.2

neighbor	0.8 bin

timestep	0.002

thermo		100

fix		1 all nve/asphere/intel

if "$p > 0"	then "run_style verlet/power"

if "$w > 0"	then "run $w"
run $w
run 10
