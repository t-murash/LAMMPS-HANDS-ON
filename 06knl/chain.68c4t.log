replicate       $x $y $z
replicate       4 $y $z
replicate       4 2 $z
replicate       4 2 2
  orthogonal box = (-16.796 -16.796 -16.796) to (117.572 50.388 50.388)
  17 by 2 by 2 MPI processor grid
  512000 atoms
  506880 bonds
  2 = max # of 1-2 neighbors
  2 = max # of special neighbors
  special bonds CPU = 0.00893784 secs
  replicate CPU = 0.0660319 secs

neighbor	0.4 bin
neigh_modify	every 1 delay 1

bond_style      fene/intel
bond_coeff	1 30.0 1.5 1.0 1.0

pair_style	lj/cut/intel 1.12
pair_modify	shift yes
pair_coeff	1 1 1.0 1.0 1.12

fix		1 all nve/intel
fix		2 all langevin 1.0 1.0 10.0 904297

thermo          1000
timestep	0.012

if "$p > 0"	then "run_style verlet/power"

if "$w > 0"	then "run $w"
run $w
run 10
