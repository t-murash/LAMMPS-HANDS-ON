pair_style	sw/intel
#pair_coeff	* * ${root}/bench/POTENTIALS/Si.sw Si
pair_coeff	* * DATA/Si.sw Si
mass            1 28.06

velocity	all create 1000.0 376847 loop geom

neighbor	1.0 bin
neigh_modify    delay 5 every 1

fix		1 all nve/intel
thermo		1000
timestep	0.001

if "$p > 0"     then "run_style verlet/power"

if "$w > 0"     then "run $w"
run $w
run 10
