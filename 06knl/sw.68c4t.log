package intel 1
package intel 0
# bulk Si via Stillinger-Weber

variable        N index on      # Newton Setting
variable        w index 10      # Warmup Timesteps
variable        t index 6200	# Main Run Timesteps
variable        m index 1       # Main Run Timestep Multiplier
variable        n index 0       # Use NUMA Mapping for Multi-Node
variable        p index 0       # Use Power Measurement

variable	x index 2
variable	y index 2
variable	z index 4

variable	xx equal 20*$x
variable	xx equal 20*2
variable	yy equal 20*$y
variable	yy equal 20*2
variable	zz equal 10*$z
variable	zz equal 10*4
variable        rr equal floor($t*$m)
variable        rr equal floor(6200*$m)
variable        rr equal floor(6200*1)
variable        root getenv LMP_ROOT

newton          $N
newton          on
if "$n > 0"     then "processors * * * grid numa"

units		metal
atom_style	atomic

lattice		diamond 5.431
