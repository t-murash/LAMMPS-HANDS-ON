region		box block 0 ${xx} 0 ${yy} 0 ${zz}
region		box block 0 40 0 ${yy} 0 ${zz}
region		box block 0 40 0 40 0 ${zz}
region		box block 0 40 0 40 0 40
create_box	1 box
