package intel 1
package intel 0
# DPD benchmark

variable        N index on      # Newton Setting
variable	w index 10	# Warmup Timesteps
variable	t index 4000	# Main Run Timesteps
variable	m index 1	# Main Run Timestep Multiplier
variable	n index 0	# Use NUMA Mapping for Multi-Node
variable	p index 0	# Use Power Measurement

variable	x index 4
variable	y index 2
variable	z index 2

variable	xx equal 20*$x
variable	xx equal 20*4
variable	yy equal 20*$y
variable	yy equal 20*2
variable	zz equal 20*$z
variable	zz equal 20*2
variable	rr equal floor($t*$m)
variable	rr equal floor(4000*$m)
variable	rr equal floor(4000*1)

newton          $N
newton          on
if "$n > 0"	then "processors * * * grid numa"

units		lj
atom_style	atomic
comm_modify     mode single vel yes

lattice		fcc 3.0
Lattice spacing in x,y,z = 1.10064 1.10064 1.10064
region		box block 0 ${xx} 0 ${yy} 0 ${zz}
region		box block 0 80 0 ${yy} 0 ${zz}
region		box block 0 80 0 40 0 ${zz}
region		box block 0 80 0 40 0 40
create_box	1 box
Created orthogonal box = (0 0 0) to (88.0514 44.0257 44.0257)
  17 by 2 by 2 MPI processor grid
create_atoms	1 box
Created 512000 atoms
  create_atoms CPU = 0.011564 secs
mass		1 1.0

velocity	all create 1.0 87287 loop geom

pair_style	dpd/intel 1.0 1.0 928948
pair_coeff	1 1 25.0 4.5

neighbor	0.5 bin
neigh_modify    delay 0 every 1

fix		1 all nve/intel
timestep	0.04

thermo			1000

if "$p > 0"		then "run_style verlet/power"

if "$w > 0"		then "run $w"
run $w
run 10
